Create a countdown loop starting at a dynamic value of 10.

let start = 10;

function countdown(count) {
    
    console.log(count);

    if (count < 1) {
        return;
    }

    countdown(count - 1);
}

function countdown2(count) {
 
    console.log(count);

    if (count > 0) {
        return countdown2(count - 1);
    }
}

countdown(start);


countdown2(8);
